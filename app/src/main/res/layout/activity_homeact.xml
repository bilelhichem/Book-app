<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".homeact"
    android:background="@color/bb"
    android:padding="20dp"
    >
<FrameLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/FrameFragment"
    android:layout_below="@id/linpre"
    android:layout_above="@id/linsec"
    >

</FrameLayout>


    <LinearLayout
        android:id="@+id/linsec"
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:orientation="horizontal"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="9dp"
        android:gravity="center"
        android:padding="0dp"
        android:background="@drawable/backedit"

        >
<androidx.cardview.widget.CardView
    android:id="@+id/cardhome"
    android:layout_width="40dp"
    android:layout_height="40dp"
    app:cardCornerRadius="13dp"
    app:cardBackgroundColor="@color/red"
    >
    <ImageView
        android:id="@+id/home"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/icons8_home_25"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="4dp"
        app:tint="@color/white">

    </ImageView>
</androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardfav"
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:cardCornerRadius="13dp"
            app:cardBackgroundColor="#222227"
            android:layout_marginLeft="35dp"
            >
            <ImageView
                android:id="@+id/fav"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/icons8_ruban_marque_page"
                android:layout_marginLeft="4dp"
                android:layout_marginTop="4dp"
                app:tint="@color/white">

            </ImageView>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardprofil"
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:cardCornerRadius="13dp"
            app:cardBackgroundColor="#222227"
            android:layout_marginLeft="35dp"
            >
            <ImageView
                android:id="@+id/profil"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/utilisateur"
                android:layout_marginLeft="4dp"
                android:layout_marginTop="4dp"
                app:tint="@color/white">

            </ImageView>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardsetting"
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:cardCornerRadius="13dp"
            app:cardBackgroundColor="#222227"
            android:layout_marginLeft="35dp"
            >
            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/reglage"
                android:layout_marginLeft="4dp"
                android:layout_marginTop="4dp"
                app:tint="@color/white">

            </ImageView>
        </androidx.cardview.widget.CardView>




    </LinearLayout>




    <LinearLayout
        android:id="@+id/linpre"
    android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:orientation="horizontal"

        >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <androidx.cardview.widget.CardView
                android:id="@+id/cardView2"
                android:layout_width="40dp"
                android:layout_height="40dp"

                app:cardBackgroundColor="#222227"
                app:cardCornerRadius="14dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ImageView
                    android:id="@+id/signout"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="4dp"
                    android:src="@drawable/signout"
                    app:tint="@color/white">

                </ImageView>
            </androidx.cardview.widget.CardView>

            <TextView

                android:id="@+id/titredepage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TEXTVIEW"
                android:textColor="@color/white"
                android:textSize="16dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/gotoprofil"
                app:layout_constraintStart_toEndOf="@+id/cardView2"
                app:layout_constraintTop_toTopOf="parent">

            </TextView>

            <androidx.cardview.widget.CardView
                android:id="@+id/gotoprofil"
                android:layout_width="40dp"
                android:layout_height="40dp"
                app:cardBackgroundColor="#222227"
                app:cardCornerRadius="14dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ImageView
                    android:id="@+id/gotl"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="5dp"
                    android:src="@drawable/_781818_account_avatar_face_man_people_icon"
                    app:tint="@color/white">

                </ImageView>
            </androidx.cardview.widget.CardView>



        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>



</RelativeLayout>